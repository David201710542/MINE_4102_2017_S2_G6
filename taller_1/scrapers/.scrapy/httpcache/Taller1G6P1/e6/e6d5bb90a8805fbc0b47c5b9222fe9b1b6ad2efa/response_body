<script>
jQuery.noConflict();

function wcdboxtype(e){
	jQuery(".wcd_option").each(function() {
		jQuery(this).removeClass();
		if (jQuery(this).attr('id') == e){
			jQuery("#"+e).addClass('wcd_option wcd_menu_on');
		} else {
			jQuery(this).addClass('wcd_option wcd_menu_off');
		}
	});
	setTimeout(function(){ 
		var seltab = jQuery("input[name='wcd_radio_tab']:checked").val();
		if (seltab == "revistas"){
			jQuery("#wcd_opt_kw").attr('checked', 'checked');
			jQuery("#wcd_opt_au").attr('disabled', 'disabled');
			jQuery("#wcd_opt_au_label").attr('style', 'color: #ddd;');
		} else {
			jQuery("#wcd_opt_au").removeAttr( "disabled" );
			jQuery("#wcd_opt_au_label").attr('style', 'color: auto;');
		}
	}, 222);
	
}

function construir_query(){
	var tab = ""; 
	var opt = ""; 
	var words = "";
	tab = jQuery("input[name='wcd_radio_tab']:checked").val();
	opt = jQuery("input[name='wcd_radio_opt']:checked").val();
	words = jQuery("input[id='wcd_query']").val();
	words = words.trim(); 
	words = words.replace(/\s+/g, ' '); 
	subformato = "";
	switch(tab){
		case "todo":
			words = opt + ":" + words;
			break;
		case "libros":
			words = opt + ":" + words + " AND x0:book";
			subformato = "Book::book_printbook";
			break;
		case "ebooks":
			words = opt + ":" + words + " AND x0:book + x4:digital";
			break;
		case "articulos":
			words = opt + ":" + words + " AND x0:artchap";
			break;
		case "videos":
			words = opt + ":" + words + " AND x0:video";
			break;
		case "tesis":
			words = opt + ":" + words + " AND mt:deg";
			break;
		case "revistas":
			words = opt + ":" + words + " AND x0:jrnl";
			break;
	}
	if (words != "" && tab != "" && opt != ""){
		jQuery('#wcd_queryString').val(words);
		jQuery('#wcd_subformat').val(subformato);
		document.wcd_consultor.submit();
	}
	return false;
}
</script> 
